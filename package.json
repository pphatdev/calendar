        "jquery": "^3.6.0",
        "postcss": "^8.4.16",
        "postcss-loader": "^7.0.1",
        "postcss-preset-env": "^7.7.2",
        "sass": "^1.54.3",
        "sass-loader": "^13.0.2",
        "style-loader": "^3.3.1",
        "tailwindcss": "^3.1.8",
