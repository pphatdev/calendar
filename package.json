        "style:watch": "npx tailwindcss -i ./src/css/index.css -o ./dist/css/index.css --watch",
        "copy:file": "copyfiles -u 1 ./src/index.html ./dist",
        "build": "rimraf ./dist && tsc && npm run copy:file && npm run style:build"
    },
